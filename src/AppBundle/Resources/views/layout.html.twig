<!doctype html>
<html lang="{{ app.request.locale }}" ng-app="{{ ng_app }}">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <title>{% block title %}{% endblock %}
    </title>
    {% block metas %}{% endblock %}
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="{{ asset('node_modules/material-design-icons/iconfont/material-icons.css') }}">
    <link rel="stylesheet" href="//cdn.jsdelivr.net/highlight.js/9.2.0/styles/monokai-sublime.min.css">
    <link rel="stylesheet" href="{{ asset('node_modules/angular-material/angular-material.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/app/css/app.min.css') }}">

    {% block stylesheets %}{% endblock %}

</head>
<body>

{% block body %}{% endblock %}

{% if app.environment == 'dev' %}
    <script src="//localhost:35729/livereload.js"></script>
{% endif %}

<script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
<script src="{{ asset('node_modules/angular/angular.min.js') }}"></script>
<script src="{{ asset('node_modules/angular-ui-router/release/angular-ui-router.min.js') }}"></script>
<script src="{{ asset('node_modules/angular-ui-router.statehelper/statehelper.min.js') }}"></script>
<script src="{{ asset('node_modules/angular-animate/angular-animate.js') }}"></script>
<script src="{{ asset('node_modules/angular-aria/angular-aria.js') }}"></script>
<script src="{{ asset('node_modules/angular-material/angular-material.js') }}"></script>
<script src="{{ asset('node_modules/underscore/underscore-min.js') }}"></script>

{% if skip_routing is not defined or not skip_routing %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
{% endif %}

{% block javascripts %}{% endblock %}
</body>
</html>
